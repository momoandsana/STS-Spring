package sample03;

public class BookDAOImpl implements BookDAO {
	
	private String subject;
	private String writer;
	private int price;
	private String date;

	// 기본 생성자
	BookDAOImpl(){
		System.out.println("BookDAO impl 호출됨");
	}

	// 매개변수를 받는 생성자 추가
	BookDAOImpl(String subject, String writer, int price, String date) {
		this.subject = subject;
		this.writer = writer;
		this.price = price;
		this.date = date;
		System.out.println("BookDAOImpl 생성자 호출됨: " + subject + ", " + writer + ", " + price + ", " + date);
	}
	
	@Override
	public void insert(BookVo book) {
		System.out.println("책 삽입: " + book);
	}
}
