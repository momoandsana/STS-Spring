package sample02;

public class Member {
	private String id;
	private String pwd;
	private int age;
	private String addr;
	
	public Member()
	{
		System.out.println("Member 기본 생성자");
	}
	public Member(String id)
	{
		this.id=id;
		System.out.println("Member(String id) 생성자 호출 id = "+id);
	}
	public Member(String id,String pwd)
	{
		this.id=id
		this.pwd=pwd;
		System.out.println("Member(Stirng id,String pwd) 생성자 호출 id = "+id+" pwd = "+pwd);
	}
	public Member(String id,int age)
	{
		this.id=id;
		this.age=age;
		System.out.println("Member(Stirng id,int age) 생성자 호출 id = "+id+" age = "+age);
	}
	public Member(String id,String pwd,int age)
	{
		this.id=id;
		this.age=age;
		System.out.println("Member(Stirng id,String pwd,int age) 생성자 호출 id = "+id+" pwd = "+pwd+ " age= "+age);
	}
	public Member(String id, String pwd, int age, String addr) {
		super();
		this.id = id;
		this.pwd = pwd;
		this.age = age;
		this.addr = addr;
	}
	
	@Override
	public String toString() {
		return "Member [id=" + id + ", pwd=" + pwd + ", age=" + age + ", addr=" + addr + "]";
	}
	
	
	
}
