package sample07;

import lombok.AllArgsConstructor;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;
import lombok.ToString;

@Setter
@Getter
@NoArgsConstructor
// 인수 없는 생성자
@AllArgsConstructor
// 모든 인수 사용하는 생성자
@ToString
public class BoardDTO {
	int no;
	String subject;
	String content;
	
}
