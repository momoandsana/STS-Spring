package sample07;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Controller;

@Controller("controller")//<bean class="" id="controller"
public class BoardController {
	@Autowired
	private BoardService boardService;
	// BoardService 타입은 하나만 있음->byType 으로 BoardServiceImpl 이 들어옴
	
	@Autowired // byType 으로 들어감
	private BoardDTO boardDTO;
	
	@Autowired // 위에와 같은 객체
	private BoardDTO boardDTO2;
	
	BoardController(){
		System.out.println("BoardController 생성자");
	};
	
	void test() {
		System.out.println("boardDTO = "+boardDTO);
		System.out.println("boardDTO2 = "+boardDTO2);
	};
}
